<%@page import="tech.ProductEntity"%>
<%@page import="java.sql.Timestamp"%>
<%@page import="java.util.List"%>
<%@page import="java.util.Map"%>

<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
    <title>Product Page</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

    <!-- JavaScript for handling category and image change -->
    <script>
        function updateCategoryImage() {
            var category = document.getElementById("category").value;
            var imageUrl;

            if (category === "Electronics") {
            	imageUrl = "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS9-bzi-brK3fMXtVI_HQQGN2K84FiUAS6_tA&s";
            } else if (category === "Furniture") {
                imageUrl = "data:image/jpeg;base64,";
            } else if (category === "Clothing") {
                imageUrl = "data:image/png;base64,";    
            } else {
                imageUrl = ""; 
            }

            document.getElementById("productImage").src = imageUrl;
        }
    </script>
</head>

<body>
    <div class="container">
        <h1>Product Management</h1>
        <form action="/saveProduct" method="POST">
            <!-- Product Name -->
            <div class="form-group">
                <label for="productName">Product Name</label>
                <input type="text" class="form-control" id="productName" name="productName" placeholder="Enter product name" required>
            </div>

            <!-- Product Category -->
            <div class="form-group">
                <label for="category">Product Category</label>
                <select class="form-control" id="category" name="category" onchange="updateCategoryImage()">
                    <option value="" disabled selected>Select a category</option>
                    <option value="Electronics">Electronics</option>
                    <option value="Furniture">Furniture</option>
                    <option value="Clothing">Clothing</option>
                </select>
            </div>

            <!-- Product Price -->
            <div class="form-group">
                <label for="price">Product Price</label>
                <input type="number" class="form-control" id="price" name="price" placeholder="Enter price" required>
            </div>

            <!-- Product Image (Dynamic based on category selection) -->
            <div class="form-group mt-3">
                <label for="productImage">Product Image</label><br>
                <img id="productImage" style="width: 200px; height: auto;" alt="Product Image">
            </div>

            <!-- Save Button -->
            <button type="submit" class="btn btn-primary mt-2">Save</button>
        </form>

        <!-- Display the Products in a Table -->
        <hr>
        <h2>Products List</h2>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Product Name</th>
                    <th>Category</th>
                    <th>Price</th>
                    <th>Image</th>
                    <th>Created Date</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                <%
                // Fetch the product details from the request (replace with your logic)
                List<ProductEntity> productList = (List<ProductEntity>) request.getAttribute("productList");
        
               

                if (productList != null && !productList.isEmpty()) {
                	  for (ProductEntity product : productList) {
                          String productName = product.getName();
                          String category = product.getCategory();
                          double price = product.getPrice();
                          String imageUrl = product.getImageUrl();
                          Timestamp createdDate = product.getCreatedDate(); // Use actual product's createdDate
                          
                          if (category.equalsIgnoreCase("Electronics")) {
                            	imageUrl = "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS9-bzi-brK3fMXtVI_HQQGN2K84FiUAS6_tA&s";
                            } else if (category.equalsIgnoreCase("Furniture")) {
                                imageUrl = "data:image/jpeg;base64,";
                            } else if (category.equalsIgnoreCase("Clothing")){
                                imageUrl = "data:image/png;base64,";    
                            } else {
                                imageUrl = ""; 
                            }
                     
                %>
                <tr>
                    <td><%= productName %></td>
                    <td><%= category%></td> <!-- Replace with product category -->
                    <td><%= price %></td> <!-- Replace with product price -->
                    <td><img src="<%= imageUrl %>" style="width: 50px; height: auto;"></td>
                    <td><%= createdDate %></td>
                    <td>
                        <a href="deleteProduct?productId=<%= product.getproductId()%>"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRNjUeKMhknDGzKYP2ZPoejzLgczDfMgaWu9g&s" alt="Delete" style="width: 20px;"></a>                  </td>
                </tr>
                <%
                    }
                } else {
                %>
                <tr>
                    <td colspan="6">No products found</td>
                </tr>
                <%
                }
                %>
            </tbody>
        </table>
    </div>
</body>
</html>